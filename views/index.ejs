<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Keystone State Basketball Camps</title>

  <!--styles-->
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

  <!--scripts-->
  <script src="/javascripts/jquery.min.js"></script>
  <script src="/javascripts/bootstrap.min.js"></script>
  <script>
    // Picture element HTML5 shiv
    document.createElement( "picture" );
  </script>
  <script src="/javascripts/picturefill.min.js" async></script>

  <!--angular scripts-->
  <script src="/javascripts/angular.js"></script>
  <script src="/javascripts/angular-mocks.js"></script>
  <script src="/javascripts/angular-resource.js"></script>
  <script src="/javascripts/angular-ui-router.js"></script>

  <!--angular app -->
  <script src="/javascripts/app/angularApp.js"></script>
  <script src="/javascripts/common/common.services.js"></script>
  <script src="/javascripts/common/mainInfo.js"></script>
  <script src="/javascripts/controllers/navController.js"></script>

</head>
<body ng-app="KeystoneStateApp">
  <main id="page-content">
  <div class="container" id="wrapper">
    <!--header-->
    <header id="nav" class="row"  ng-controller="NavController as vm">
      <nav class="navbar navbar-default navbar-static-top" id="top-navbar">
        <div class="navbar-header">
          <a class="navbar-brand" ui-sref="{{vm.header.logo.link}}">
            <img ng-repeat="logo in vm.header.logo.picture" class="{{logo.classNames}}" alt="Brand" ng-src="{{logo.pictureUrl}}"/>
          </a>
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#menu" aria-expanded="false" aria-controls="menu">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span> <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="menu">

          <ul class="nav navbar-nav navbar-right">
            <li ng-repeat="a in vm.header.links">
              <a href="{{a.href}}">{{a.link}}</a>
            </li>
          </ul>
        </div>
      </nav>
    </header>
    <!--end of header-->
    <ui-view></ui-view>
  </div>
</main>
  <footer>
  <div class="container">
    <section class="row" id="footer-section">
      <!--footer-sub-->
      <div id="footer-sub" class="col-md-12">
        <section class="col-md-12" id="sub-1">
          <section id="footer-links" class="col-sm-12 col-md-6">
            <ul class="nav navbar-nav pull-left">
              <li><a href="#">HOME</a></li>
              <li><a href="#">SITEMAP</a></li>
              <li><a href="#">FAQ</a></li>
              <li><a href="#">PRIVACY POLICY</a></li>
            </ul>
          </section>
          <section id="email-phone" class="col-sm-12 col-md-6">
            <ul class="list-inline pull-right" id="footer-contact-1">
              <li><span class="fa fa-phone fa-lg footer-contact"></span> +1 731 555 5555</li>
              <li><span class="fa fa-envelope fa-lg footer-contact"></span><a id="email" href="mailto:keystonestatecamps@hotmail.com"> keystonestatecamps@hotmail.com</a> </li>
            </ul>
          </section>
        </section>
        <section class="col-md-12" id="sub-2">
          <p class="col-md-5 pull-left"> Â© 2015 Keystone State Basketball Camp ALL RIGHTS RESERVED</p>
          <section class="col-md-2" id="fb-yt">
            <a href="#" class=""><span class="social-sprite" id="facebook">&nbsp;</span></a>
            <a href="#" class=""><span class="social-sprite" id="youtube">&nbsp;</span></a>
          </section>

          <div id="development" class="col-md-5 text-center">
            <p style="display: inline-block;">Developed by</p>
            <div></div>
          </div>
        </section>


      </div>
      <!--end footer-sub-->
    </section>
  </div>
</footer>
</body>
</html>

