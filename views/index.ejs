<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Keystone State Basketball Camps</title>

  <!--styles-->
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/_css/home.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/_css/camps.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/_css/contact.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/_css/camp.listOfPersons.css">
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

  <!--scripts-->
  <script src="/javascripts/jquery.min.js"></script>
  <script src="/javascripts/bootstrap.min.js"></script>
  <script>
    // Picture element HTML5 shiv
    document.createElement( "picture" );
  </script>
  <script src="/javascripts/picturefill.min.js" async></script>

  <!--angular scripts-->
  <script src="/javascripts/angular.js"></script>
  <script src="/javascripts/angular-mocks.js"></script>
  <script src="/javascripts/angular-resource.js"></script>
  <script src="/javascripts/angular-ui-router.js"></script>

  <!--angular app -->
  <script src="/javascripts/app/angularApp.js"></script>
  <script src="/javascripts/common/common.services.js"></script>
  <script src="/javascripts/common/mainInfo.js"></script>
  <script src="/javascripts/common/dataResourceMock.js"></script>
  <script src="/javascripts/common/dataResource.js"></script>

  <!--controllers-->
  <script src="/javascripts/controllers/templateController.js"></script>
  <script src="/javascripts/controllers/homeController.js"></script>
  <script src="/javascripts/controllers/campsController.js"></script>
  <script src="/javascripts/controllers/aboutController.js"></script>
  <script src="/javascripts/controllers/contactController.js"></script>
  <script src="/javascripts/controllers/signupController.js"></script>
  <script src="/javascripts/controllers/miscController.js"></script>

</head>
<body ng-app="KeystoneStateApp">
  <main id="page-content">
  <div class="container" id="wrapper">
    <!--header-->
    <header id="nav" class="row"  ng-controller="TemplateController as vm">
      <nav class="navbar navbar-default navbar-static-top" id="top-navbar">
        <div class="navbar-header">
          <a class="navbar-brand" ui-sref="home">
            <img ng-repeat="logo in vm.header.logo.picture" class="{{logo.classNames}}" alt="Brand" ng-src="{{logo.pictureUrl}}"/>
          </a>
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#menu" aria-expanded="false" aria-controls="menu">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span> <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="menu">

          <ul class="nav navbar-nav navbar-right">
            <li ng-repeat="a in vm.header.links">
              <a ui-sref="{{a.href}}">{{a.link}}</a>
            </li>
          </ul>
        </div>
      </nav>
    </header>
    <!--end of header-->
    <ui-view></ui-view>
  </div>
</main>
  <footer ng-controller="TemplateController as vm">
  <div class="container">
    <section class="row" id="footer-section">
      <!--footer-sub-->
      <div id="footer-sub" class="col-md-12">
        <section class="col-md-12" id="sub-1">
          <section id="footer-links">
            <ul class="nav navbar-nav">
              <li ng-repeat="link in vm.footer.footerLinks">
                <a ui-sref="{{link.url}}">{{link.page}}</a>
              </li>
            </ul>
          </section>
          <section id="email-phone" >
            <ul id="footer-contact-1">
              <li ng-repeat="item in vm.footer.footerContact">
                <a href="{{item.url}}" class="email"><span class="{{item.className}}"></span>&nbsp;{{item.text}}</a>
              </li>
            </ul>
          </section>
        </section>
        <section id="sub-2">
          <p>{{vm.footer.copyrightText}}</p>
          <!---->
          <section id="fb-yt">
            <a ng-repeat="item in vm.footer.socialMedia" href="{{item.url}}">
              <span class="{{item.className}}" id="{{item.id}}">&nbsp;</span>
            </a>
          </section>

          <div id="{{vm.footer.devTag.id}}">
            <p>{{vm.footer.devTag.content}}
              <span id="{{vm.footer.devTag.signature[0]}}">{{vm.footer.devTag.signature[1]}}</span>
            </p>
          </div>
        </section>
      </div>
      <!--end footer-sub-->
    </section>
  </div>
</footer>
</body>
</html>

